{
  "framework": "create-react-app",
  "buildCommand": "yarn build:vercel",
  "outputDirectory": "build",
  "installCommand": "yarn install",
  "env": {
    "REACT_APP_USE_PRODUCTION": "true"
  },
  "build": {
    "env": {
      "REACT_APP_USE_PRODUCTION": "true"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}